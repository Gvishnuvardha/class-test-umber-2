#include <stdio.h>
#include <stdlib.h>



int main() {
     int arr[100], rev_arr[100];
   int i, j, n;

   printf("Enter the size of array: ");
   scanf("%d", &n);

   printf("Enter the elements of array: ");
   for (i = 0; i < n; i++) {
      scanf("%d", &arr[i]);
   }

   // reverse the array
   for (i = n-1, j = 0; i >= 0; i--, j++) {
      rev_arr[j] = arr[i];
   }

   // print the reversed array
   printf("Reversed array: ");
   for (i = 0; i < n; i++) {
      printf("%d ", rev_arr[i]);
   }
    return 0;



}
